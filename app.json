{
  "expo": {
    "name": "EventX-React-Native",
    "slug": "eventx-react-native",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./src/assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.eventx"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.eventx"
    },
    "web": {
      "favicon": "./src/assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "29657a98-b850-4614-ac0e-384916fa7b8c"
      }
    },
    "owner": "haroon-tahir",
    "plugins": [
      [
        "react-native-auth0",
        {
          "domain": "dev-0wrwesnq6iraecr0.us.auth0.com"
        }
      ]
    ]
  }
}
